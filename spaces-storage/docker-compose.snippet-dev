
  spaces-storage:
    image: spaces-storage
    build: 
      context: ./spaces-storage
      dockerfile: Dockerfile
    container_name: spaces-storage
    ports:
     - "4000:80"
    environment:
      - PORT=80
      - AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
      - AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}

    restart: "no"

